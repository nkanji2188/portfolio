        li.gnav__list: a.gnav__link(href="#sec1") 01. k.n website
        li.gnav__list: a.gnav__link(href="#sec2") 02. design
        li.gnav__list: a.gnav__link(href="#sec3") 03. group work
        li.gnav__list: a.gnav__link(href="#sec4") 04. jQuery
        li.gnav__list: a.gnav__link(href="#sec5") 05. corporate
        li.gnav__list: a.gnav__link(href="#sec6") first view-01
        li.gnav__list: a.gnav__link(href="#sec-pr") self appeal
        li.gnav__list: a.gnav__link(href="#footer") contact
    main.main      
      
      //- 新規仮追加
      section.sec-new
